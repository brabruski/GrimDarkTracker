<UserControl x:Class="GrimDarkTracker.Views.MaelstromView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GrimDarkTracker.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel HorizontalAlignment="stretch" VerticalAlignment="Top" Orientation="Horizontal">
            <ContentControl Content="{Binding PlayDeck}"/>
        </StackPanel>
    </Grid>
</UserControl>
